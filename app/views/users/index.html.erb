<table class='table'>
 <thead>
   <tr>
     <th>image</th>
     <th>name</th>
     <th colspan="4"></th>
   </tr>
 </thead>

  <tbody>
<% @users.each do |user| %>

  <tr>
    <td><%= image_tag user.get_profile_image(100,100) %>
    </td>
    <td><p><%= user.name %></p></td>
    <td><%= link_to "Show", user_path(user.id) %></td>
  </tr>
<% end %>
 </tbody>
</table>