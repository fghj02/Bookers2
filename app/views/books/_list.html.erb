<% books.each do |book| %>
<div>
  <%= link_to book_path(book.id) do %>
    <%= image_tag book.get_image %>
  <% end %>
  <p>image：<%= image_tag book.user.get_profile_image(100,100) %></p>
  <p>title：<%= book.book_name %></p>
  <p>body：<%= book.book_body %></p>
  <p>user name：<%= book.user.name %></p>
</div>
<% end %>
<%= paginate books %>